}
int n = 1;
while(true)
{
// computing the triangular number for the number n
int triangularNum = (n * (n + 1)) / 2;
// computing the centered square number
// which the multiplication of triangular number
// with the number 4 + 1.
int ans = 4 * triangularNum;
ans = ans + 1;
if(ans == inputNum)
{
return true;
}
else if(ans > inputNum)
{
