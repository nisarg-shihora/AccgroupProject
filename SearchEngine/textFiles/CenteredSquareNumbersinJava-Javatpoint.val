{
public int findCenteredSqureNum(int num) 
{
// handling the base case
if(num == 1)
{
    return num;
}
// recursively computing the centered square number
return findCenteredSqureNum(num - 1) + 4 * (num - 1);
}
// Main method
public static void main(String[] argvs) 
{
// creating an object of the class CenteredSquareNumber
CenteredSquareNumber obj = new CenteredSquareNumber();
// computing the first 10 centered square numbers
int num = 10;
System.out.println("The first " + num + " Centered Square Number are: ");
for(int i = 1; i <= 10; i++)
{
int ans = obj.findCenteredSqureNum(i);
