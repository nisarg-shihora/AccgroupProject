public class CenteredSquareNumber4
{
// a method that check whether the input number a 
// is a centered square number or not
public boolean isCenteredSqrNum(int inputNum)
{
// 1 is a centered square number
if(inputNum == 1)
{
return true;
}
int n = 1;
while(true)
{
// computing the triangular number for the number n
int triangularNum = (n * (n + 1)) / 2;
// computing the centered square number
// which the multiplication of triangular number
// with the number 4 + 1.
int ans = 4 * triangularNum;
ans = ans + 1;
if(ans == inputNum)
{
return true;
}
else if(ans > inputNum)
{
return false;
}
// look for the next triangular number
// hence, increment n by 1
n = n + 1;
}
}
// Main method
public static void main(String[] argvs) 
{
// creating an object of the class CenteredSquareNumber4
CenteredSquareNumber4 obj = new CenteredSquareNumber4();
// input array
int inputArr[] = {4, 5, 8, 13, 15, 25, 40, 41};
// size of the input array
int size = inputArr.length;
for(int i = 0; i < size; i++)
{
if(obj.isCenteredSqrNum(inputArr[i]))
{
System.out.println(inputArr[i] + " is a centered square number.");
}
else
{
System.out.println(inputArr[i] + " is not a centered square number.");
}
}
}
}
