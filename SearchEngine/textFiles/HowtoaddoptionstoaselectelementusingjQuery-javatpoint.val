"https://code.jquery.com/jquery-3.3.1.min.js">
</script>
	<script type="text/javascript">
		function addOption() {
			optionText = 'Premium';
			optionValue = 'premium';
			$('#select1').append(`<option value="${optionValue}">
		${optionText}
		</option>`);
		}
	</script>
</body>
</html>
