// import required classes and package if any
import java.util.*;

// create IntSummaryStatisticsExample1 class to understand the use of accept() method
public class IntSummaryStatisticsExample1 {
	
	// main() method start
	public static void main(String[] args){
		
		int n;
		
		// create an instance of IntSummaryStatistics class
		IntSummaryStatistics obj = new IntSummaryStatistics();

		// create an empty list of Integer
		List<Integer> list = new ArrayList<Integer>();
		
		// create and instance of Scanner class
		Scanner sc = new Scanner(System.in);
		
		System.out.println("How many integers you want to add to the IntSummaryStatistics object");
		n = sc.nextInt();
		
		for(int i = 0; i < n;  i++) {
			System.out.println("Enter "+(i+1)+" element.");
			list.add(sc.nextInt());
		}

		// close Scanner class object
		sc.close();
		
		// create an Iterator of integer with list elements
		Iterator<Integer> iterator = list.listIterator();
		
		// repeat iterator elements
		while (iterator.hasNext()) {
			
			// use accept() method for adding the integers to the IntSummaryStatistics object
			obj.accept(iterator.next());
		}

		System.out.println(obj.toString());
	}
}
