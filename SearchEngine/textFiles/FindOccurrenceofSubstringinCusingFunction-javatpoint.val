    scanf ( " % [ ^ \ n ] s " , str ) ;
    l1 = strlen ( str ) ;
    printf ( " \ n enter a substring : " ) ;
    scanf ( " % [ ^ \ n ] s " , sub ) ;
    l2 = strlen ( sub ) ;
    for ( i = 0 ; i < l1 ; )
    {
        j = 0 ;
        count = 0 ;
        while ( ( str [ i ] = = sub [ j ] ) )
        {
            count + + ;
            i + + ;
            j + + ;
        }
        if ( count = = l2 )
        {
            count1 + + ;                                   
            count = 0 ;
        }
        else
            i + + ;
    }    
    printf ( " % s occurs % d times in % s " , sub , count1 , str ) ;
}
