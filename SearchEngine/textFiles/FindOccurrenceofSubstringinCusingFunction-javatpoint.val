/* 
 * C Program To Count the Occurrence of a Substring in String 
 */
# include < stdio . h >
# include < string . h >
# include < stdlib >
# include < conio . h >
char str [ 100 ] , sub [ 100 ] ;
int count = 0 , count1 = 0 ;
void main()
{
    int i , j , l , l1 , l2 ;
