public class RotateArray1
{
// It rotates the array elements by r.
public void leftRotate(int input[], int r, int s)
{
int j, k;
// the array will remain the same when size of the 
// input array is equal to the number of times the 
// the array is rotated. Also, if we do not rotate the array 
// at all we get the same array
if(r == 0 || r == s)
{
return;
}
// If the number of elements that have to be rotated is
// greater than the array size
if(r > s)
{
r = r % s;
}
j = r;
k = s - r;
while (j != k)
{
if(j < k) // if A is of short in size
{
swap(input, r - j, r + k - j, j);
k = k - j;
}
else // If B is short in size
{
swap(input, r - j, r, k);
j = j - k;
}
}
// In the end, do the block swap A and B
swap(input , r - j, r, j);
}
// The method swaps the r elements
// beginning at the index f with r elements
// starting at the index g
public void swap(int input[], int f, int g, int r)
{
int k;
int temp;
for(k = 0; k < r; k++)
{
temp = input[f + k];
input[f + k] = input[g + k];
input[g + k] = temp;
}
}
